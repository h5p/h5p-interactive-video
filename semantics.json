      {
        "name": "preventSkippingMode",
        "type": "select",
        "label": "Disable navigation",
        "importance": "low",
        "description": "These options will disable user video navigation as specified.",
        "optional": true,
        "options": [
          {
            "label": "None",
            "value": "none"
          },
          {
            "label": "Forward",
            "value": "forward"
          },
          {
            "label": "Forward and backward",
            "value": "both"
          }
        ],
        "default": "none"
      },
      {
        "name": "deactivateSound",
