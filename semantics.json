      {
        "name": "summary",
        "type": "group",
        "label": "Summary task",
        "importance": "high",
        "fields": [
          {
            "name": "task",
            "type": "library",
            "options": [
              "H5P.Summary 1.7"
            ],
            "default": {
              "library": "H5P.Summary 1.7",
              "params": {}
            }
          },
          {
            "name": "displayAt",
            "type": "number",
            "label": "Display at",
            "description": "Number of seconds before the video ends.",
            "default": 3
          }
        ]
      }
    ]
